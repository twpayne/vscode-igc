{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "IGC",
    "patterns": [
        {
            "include": "#records"
        }
    ],
    "repository": {
        "records": {
            "patterns": [
                {
                    "name": "record.a.igc",
                    "match": "^(A)(...)([^\\-]*)(-.*)?$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        },
                        "3": {
                            "name": "storage.type.igc"
                        },
                        "4": {
                            "name": "entity.name.igc"
                        }
                    }
                },
                {
                    "name": "record.b.igc",
                    "match": "^(B)(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{3})([NS])(\\d{3})(\\d{2})(\\d{3})([EW])([AV])(\\d{5}|-\\d{4})(\\d{5}|-\\d{4})(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "storage.type.igc"
                        },
                        "3": {
                            "name": "entity.name.igc"
                        },
                        "4": {
                            "name": "variable.name.igc"
                        },
                        "5": {
                            "name": "storage.type.igc"
                        },
                        "6": {
                            "name": "entity.name.igc"
                        },
                        "7": {
                            "name": "variable.name.igc"
                        },
                        "8": {
                            "name": "string.quoted.igc"
                        },
                        "9": {
                            "name": "storage.type.igc"
                        },
                        "10": {
                            "name": "entity.name.igc"
                        },
                        "11": {
                            "name": "variable.name.igc"
                        },
                        "12": {
                            "name": "string.quoted.igc"
                        },
                        "13": {
                            "name": "string.quoted.igc"
                        },
                        "14": {
                            "name": "storage.type.igc"
                        },
                        "15": {
                            "name": "entity.name.igc"
                        },
                        "16": {
                            "name": "variable.name.igc"
                        }
                    }
                },
                {
                    "name": "record.c.line1.igc",
                    "match": "^(C)(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{2})(\\d{4})(\\d{2})(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "variable.name.igc"
                        },
                        "3": {
                            "name": "entity.name.igc"
                        },
                        "4": {
                            "name": "storage.type.igc"
                        },
                        "5": {
                            "name": "storage.type.igc"
                        },
                        "6": {
                            "name": "entity.name.igc"
                        },
                        "7": {
                            "name": "variable.name.igc"
                        },
                        "8": {
                            "name": "variable.name.igc"
                        },
                        "9": {
                            "name": "entity.name.igc"
                        },
                        "10": {
                            "name": "storage.type.igc"
                        },
                        "11": {
                            "name": "string.quoted.igc"
                        },
                        "12": {
                            "name": "storage.type.igc"
                        }
                    }
                },
                {
                    "name": "record.c.point.igc",
                    "match": "^(C)(\\d{2})(\\d{2})(\\d{3})([NS])(\\d{3})(\\d{2})(\\d{3})([EW])(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "storage.type.igc"
                        },
                        "3": {
                            "name": "entity.name.igc"
                        },
                        "4": {
                            "name": "variable.name.igc"
                        },
                        "5": {
                            "name": "string.quoted.igc"
                        },
                        "6": {
                            "name": "storage.type.igc"
                        },
                        "7": {
                            "name": "entity.name.igc"
                        },
                        "8": {
                            "name": "variable.name.igc"
                        },
                        "9": {
                            "name": "string.quoted.igc"
                        },
                        "10": {
                            "name": "string.quoted.igc"
                        }
                    }
                },
                {
                    "name": "record.h.hfdte.igc",
                    "match": "^(H)(F)(DTE)(\\d\\d)(\\d\\d)(\\d\\d)(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        },
                        "3": {
                            "name": "storage.type.igc"
                        },
                        "4": {
                            "name": "variable.name.igc"
                        },
                        "5": {
                            "name": "entity.name.igc"
                        },
                        "6": {
                            "name": "storage.type.igc"
                        },
                        "7": {
                            "name": "invalid.igc"
                        }
                    }
                },
                {
                    "name": "record.h.hffxa.igc",
                    "match": "^(H)(F)(FXA)(\\d+)(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        },
                        "3": {
                            "name": "storage.type.igc"
                        },
                        "4": {
                            "name": "entity.name.igc"
                        },
                        "5": {
                            "name": "invalid.igc"
                        }
                    }
                },
                {
                    "name": "record.h.igc",
                    "match": "^(H)(.)([0-9A-Z]{3})([^:]*)(:)?(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        },
                        "3": {
                            "name": "storage.type.igc"
                        },
                        "4": {
                            "name": "entity.name.igc"
                        },
                        "5": {
                            "name": "variable.name.igc"
                        },
                        "6": {
                            "name": "storage.type.igc"
                        }
                    }
                },
                {
                    "name": "record.g.igc",
                    "match": "^(G)(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        }
                    }
                },
                {
                    "name": "record.i.igc",
                    "begin": "^(I)(\\d{2})",
                    "beginCaptures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#ifields"
                        }
                    ],
                    "end": "$"
                },
                {
                    "name": "record.l.igc",
                    "match": "^(L)(...)(.*)$",
                    "captures": {
                        "1": {
                            "name": "keyword.control.igc"
                        },
                        "2": {
                            "name": "string.quoted.igc"
                        },
                        "3": {
                            "name": "comment.igc"
                        }
                    }
                }
            ]
        },
        "ifields": {
            "patterns": [
                {
                    "name": "record.i.field.igc",
                    "match": "(\\d{2})(\\d{2})(.{3})",
                    "captures": {
                        "1": {
                            "name": "string.quoted.igc"
                        },
                        "2": {
                            "name": "storage.type.igc"
                        },
                        "3": {
                            "name": "entity.name.igc"
                        }
                    }
                }
            ]
        }
    },
    "scopeName": "source.igc"
}
